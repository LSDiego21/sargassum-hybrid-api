<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://schemas.xmlsoap.org/wsdl/"
             xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
             xmlns:tns="http://www.qroo.gob.mx/wsdl/SargassumTelemetry.wsdl"
             xmlns:xsd="http://www.w3.org/2001/XMLSchema"
             name="SargassumTelemetryService"
             targetNamespace="http://www.qroo.gob.mx/wsdl/SargassumTelemetry.wsdl">
    <message name="GetReportRequest">
        <part name="municipio" type="xsd:string"/>
    </message>
    <message name="GetReportResponse">
        <part name="playa" type="xsd:string"/>
        <part name="nivel_inundacion" type="xsd:string"/>
        <part name="biomasa_estimada_tons" type="xsd:int"/>
    </message>
    <portType name="TelemetryPortType">
        <operation name="ConsultarRiesgoPlaya">
            <input message="tns:GetReportRequest"/>
            <output message="tns:GetReportResponse"/>
        </operation>
    </portType>
    <binding name="TelemetryBinding" type="tns:TelemetryPortType">
        <soap:binding style="rpc" transport="http://schemas.xmlsoap.org/soap/http"/>
        <operation name="ConsultarRiesgoPlaya">
            <soap:operation soapAction="ConsultarRiesgoPlaya"/>
            <input><soap:body use="literal"/></input>
            <output><soap:body use="literal"/></output>
        </operation>
    </binding>
    <service name="SargassumTelemetryService">
        <port name="TelemetryPort" binding="tns:TelemetryBinding">
            <soap:address location="https://sargassum-hybrid-api.onrender.com/wsdl"/>
        </port>
    </service>
</definitions>